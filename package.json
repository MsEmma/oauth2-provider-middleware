{
  "name": "oauth2-provider-middleware",
  "version": "0.0.1",
  "description": "Middleware implementing an OAuth2 provider, with pluggable storage",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "concurrently \"npm:watch-*\"",
    "watch-prettier": "onchange '**/!(__generated__)/*.js' -- prettier --write {{changed}}",
    "watch-eslint": "onchange '**/!(__generated__)/*.js' -- eslint {{changed}}",
    "watch-test": "npm run test -- --watch",
    "sample": "node sample"
  },
  "keywords": [
    "middleware",
    "oauth2",
    "express",
    "authentication"
  ],
  "author": "chris@uber5.com",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "express": "^4.16.4",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "onchange": "^5.2.0",
    "prettier": "1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{js}": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {}
}
